package org.foobarspam.main;

public class Main {

	public static void main(String[] args) {
		//int y char
		int x;
		x = 1;
		switch(x)
		{
		case 0:
		{
			System.out.println("Esta es la opcion 0");
			break;
		}
		case 1:
		{
			System.out.println("Esta es la opcion 1");
			break;
		}
		}

	}

}
